{
    "version": "next",
    "compilerOptions": {
        "declaration": true,
        "target": "es5",
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "jsx": "react",
        "outDir": "./build"
    },
    "filesGlob": [
        "./typings/**/*.ts",
        "./src/**/*.ts",
        "./src/**/*.tsx"
    ],
    "files": [
        "./typings/jquery/jquery.d.ts",
        "./typings/null.ts",
        "./typings/threejs/detector.d.ts",
        "./typings/threejs/three-canvasrenderer.d.ts",
        "./typings/threejs/three-copyshader.d.ts",
        "./typings/threejs/three-css3drenderer.d.ts",
        "./typings/threejs/three-editorcontrols.d.ts",
        "./typings/threejs/three-effectcomposer.d.ts",
        "./typings/threejs/three-FirstPersonControls.d.ts",
        "./typings/threejs/three-maskpass.d.ts",
        "./typings/threejs/three-orbitcontrols.d.ts",
        "./typings/threejs/three-projector.d.ts",
        "./typings/threejs/three-renderpass.d.ts",
        "./typings/threejs/three-shaderpass.d.ts",
        "./typings/threejs/three-trackballcontrols.d.ts",
        "./typings/threejs/three-transformcontrols.d.ts",
        "./typings/threejs/three-vrcontrols.d.ts",
        "./typings/threejs/three-vreffect.d.ts",
        "./typings/threejs/three.d.ts",
        "./typings/tsd.d.ts",
        "./src/CustomShaderRotatingCube.ts",
        "./src/main.ts",
        "./src/MainUI.ts",
        "./src/MultiMaterialObject.ts",
        "./src/RotatingCube.ts",
        "./src/Sofa.ts",
        "./src/SofaShader.ts",
        "./src/SofaShaderSphere.ts",
        "./src/type.ts",
        "./src/VaseShaderSphere.ts",
        "./src/WonderCamera.ts",
        "./src/WonderObjLoader.ts",
        "./src/WonderOperator.ts",
        "./src/WonderRenderer.ts",
        "./src/WonderScene.ts",
        "./src/WonderVR.ts"
    ],
    "exclude": [],
    "atom": {
        "rewriteTsconfig": true
    }
}
